{
    "extends": ["tslint:recommended"],
    "rules": {
      "object-literal-sort-keys": false,
      "indent": [true, "spaces", 2],
      "quotemark": [true, "single", "jsx-double"],
      "jsx-boolean-value": false,
      "no-console": false,
      "no-trailing-whitespace":false,
      "forin":false,
      "radix":false,
      "variable-name":false,
      "interface-name": false
    }
}